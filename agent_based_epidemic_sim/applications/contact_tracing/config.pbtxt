      buckets {
        int_value: 4
        count: 0.4
      }
      buckets {
        int_value: 5
        count: 0.1
      }
    }
  }
  transmissibility: .95
  distancing_policy {
    stages {
      start_time {
        seconds: 1728000
      }
      essential_worker_fraction: 0.2
    }
    stages {
      start_time {
        seconds: 5184000
      }
      essential_worker_fraction: 1.0
    }
  }
  step_size {
    seconds: 86400
  }
  num_steps: 100
}
tracing_policy {
  test_validity_duration {
    seconds: 604800
  }
  contact_retention_duration {
    seconds: 1209600
  }
  quarantine_duration {
    seconds: 1209600
  }
  test_latency {
    seconds: 86400
  }
